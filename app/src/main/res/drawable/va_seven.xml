<?xml version="1.0" encoding="utf-8"?>
<vector
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:width="15dp"
        android:height="15dp"
        android:viewportWidth="19"
        android:viewportHeight="19"
        >

    <group
            android:name="outer_group"
            android:translateX="9.5"
            android:translateY="9.5">
        <path
                android:name="outer_path"
                android:strokeWidth="@fraction/sign_in_number_path_stroke_width"
                android:strokeColor="@color/number_dark"
                android:trimPathEnd="@fraction/sign_in_outer_path_trim_path_end"
                android:pathData="M 0,0 m 9,0 a 9,9 0 1,0 -18,0 a 9,9 0 1,0 18,0 L40,0"/>

    </group>

    <group
            android:name="inner_group"
            android:translateX="9.5"
            android:translateY="9.5">
        <path
                android:name="inner_path"
                android:fillColor="@color/number_bright"
                android:pathData="@string/sign_in_inner_path"/>
    </group>


    <group
            android:name="number_group"
            android:translateX="4.4"
            android:translateY="5.4">
        <path
                android:name="number_path"
                android:pathData="@string/sign_in_path_seven"
                android:strokeColor="@color/number_dark"
                android:strokeWidth="@fraction/sign_in_number_path_stroke_width"/>
    </group>


</vector>